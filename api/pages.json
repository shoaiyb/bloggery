{% for page in site.pages %}
{% if page.title != null %}
{
  "{{ page.title | jsonify }}": {
    "title": "{{ page.title | jsonify }}",
    "content": "{{ page.content | jsonify }}",
    "date": "{{ page.date | jsonify }}",
    "categories": {{ page.categories | jsonify }},
    "tags": {{ page.tags | jsonify }},
    "url": "{{ page.url | jsonify }}",
    "id": "{{ page.id | jsonify }}"
}
{% endif %}
{% endfor %}
