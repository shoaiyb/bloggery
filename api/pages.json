---
layout: null
---
{
{% for page in site.pages %}
{% if page.title != null && page.url != contains "/page/" %}
{% if forloop.last %}
  {{ page.title | jsonify }}: {
    "title": {{ page.title | jsonify }},
    "content": {{ page.content | jsonify }},
    "image": {{ page.image | jsonify }},
    "date": {{ page.date | jsonify }},
    "url": {{ page.url | jsonify }},
    "id": {{ page.id | jsonify }}
  }
{% else %}
  {{ page.title | jsonify }}: {
    "title": {{ page.title | jsonify }},
    "content": {{ page.content | jsonify }},
    "image": {{ page.image | jsonify }},
    "date": {{ page.date | jsonify }},
    "url": {{ page.url | jsonify }},
    "id": {{ page.id | jsonify }}
  },
{% endif %}
{% endif %}
{% endfor %}
}
